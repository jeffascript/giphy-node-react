{
  "name": "portatask",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test1": "echo \"Error: no test specified\" && exit 1",
    "local": "ts-node server.ts",
    "dev1": "ts-node-dev --respawn --pretty --transpile-only ./src/server.ts",
    "start": "node build/index.js",
    "dev": "nodemon --exec 'ts-node' ./src/index.ts",
    "clean": "rm -rf build",
    "build": "npm run-script clean && tsc",
    "build_start": "npm run-script build && npm run-script start",
    "test": "NODE_ENV=test mocha -r ts-node/register tests/** --timeout 10000 --exit",
    "lint:eslint": "./node_modules/.bin/eslint --ignore-path .eslintignore -- .",
    "lint:eslint:fix": "./node_modules/.bin/eslint --ignore-path .eslintignore --fix -- ."
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [],
  "author": "Jeff",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.3.0",
    "express-validation": "^3.0.6",
    "express-validator": "^6.12.1",
    "redis": "^3.1.2",
    "ts-node-dev": "^1.1.8"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.9",
    "@types/redis": "^2.8.28",
    "@types/validator": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
